<?xml version="1.0" encoding="UTF-8"?>
<!--Format:
name   => name of the animation sequence
start  => starting frame
end    => ending frame
loop   => looping animation (default)
-->
<animation name="hunter">
    <sequence name="walk-down"  start="0" end="2"  loop="true" />
    <sequence name="walk-left"  start="3" end="5"  loop="true" />
    <sequence name="walk-right" start="6" end="8"  loop="true" />
    <sequence name="walk-up"    start="9" end="11" loop="true" />
</animation>

