<?xml version="1.0" encoding="UTF-8"?>
<!--Format:
name   => name of the animation sequence
start  => starting frame
end    => ending frame
loop   => looping animation (default)
-->
<animation name="monkey">
    <sequence name="walk-left"  start="10" end="11" loop="true" />
    <sequence name="walk-right" start="12" end="13" loop="true" />
    <sequence name="push-left"  start="6" end="7" loop="true" />
    <sequence name="push-right" start="8" end="9" loop="true" />
    <sequence name="jump-right" start="5" end="5" loop="false" />
    <sequence name="jump-left"  start="4" end="4" loop="false" />
    <sequence name="arms-up"    start="2" end="2" loop="false" />
    <sequence name="dead"       start="3" end="3" loop="false" />
    <sequence name="idle"       start="0" end="1" loop="true" />
</animation>

